{
  "version": "0.0.1",
  "name": "ude-remover",
  "description": "Chrome extension for removing the unwanted elements from page",
  "homepage": "https://chrome.google.com/webstore/detail/fasttyping/dgibeimfinglbfgimhiffmflgimlfffl",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/ude-remover"
  },
  "author": "@ndaidong",
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "setup": "node scripts/setup",
    "lint": "eslint src/js scripts",
    "nsp": "nsp check --output checkstyle",
    "test": "npm run lint && npm run nsp",
    "release": "npm test && node scripts/release"
  },
  "devDependencies": {
    "eslint": "3.x.x",
    "eslint-config-ggc": "1.x.x",
    "mkdirp": "0.x.x",
    "nsp": "2.x.x"
  },
  "keywords": [
    "chrome",
    "offline",
    "extension",
    "apps",
    "remover",
    "tool",
    "util"
  ],
  "builder": {
    "vendorDir": "src/vendor",
    "css": {},
    "javascript": {
      "bella": "https://raw.githubusercontent.com/ndaidong/bella.js/master/dist/bella.min.js",
      "doc": "https://raw.githubusercontent.com/ndaidong/doc/master/dist/doc.min.js"
    }
  },
  "license": "MIT"
}
